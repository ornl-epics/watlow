####  Macro Definitions  ####
# (s)	- Beam Line
# (ta)	- Target Area
# (ss)	- Device Name
# (i)	- Channel or Index
# (PSvA)- Process Source Value A
# (PSvB)- Process Source Value B
# (PoFS)- Process Offset
# (Pov)	- Process Output Value
# (PoE)	- Process Output Error
# (PFn)	- Process Function
# (Punt)- Pressure Units
# (Aunt)- Altitude Units
# (bPr)	- Barometric Pressure
# (PvF)	- Process Value Filter


#=========== Process Value Menu ===========
# =========== Inputs
# Process Source Value A
record(ai, "$(s):$(ta):$(ss):$(mod):PrcsSrcVA$(ch)") 
{
	field(SCAN,	"I/O Intr")
	field(PINI,	"YES")
	field(DTYP,	"asynFloat64")
	field(INP,	"@asyn(watlow_Prc_Num_1.$(mod),$(PSvA=0),100)FLOAT32_BE")
	field(EGU,	"째")
	field(PREC,	"3")
	field(ADEL,	"0.1")
	field(MDEL,	"0.1")
}

