}

# Linearization Error
record(mbbi, "$(s):$(ta):$(ss):1:LinError$(i)") 
{
	field(SCAN,	"1 second")
	field(DTYP,	"asynUInt32Digital")
	field(INP,	"@asynMask(mod_enum.1,$(LEr=3614),0xFF,1000)UINT16")
	field(ZRST,	"None")
